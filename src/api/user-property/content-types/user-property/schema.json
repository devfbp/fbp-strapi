{
  "kind": "collectionType",
  "collectionName": "user_properties",
  "info": {
    "singularName": "user-property",
    "pluralName": "user-properties",
    "displayName": "User_Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-type.property-type"
    },
    "Price": {
      "type": "string"
    },
    "Bedroom": {
      "type": "string"
    },
    "Bathroom": {
      "type": "string"
    },
    "Parking": {
      "type": "string"
    },
    "Area_Sqft": {
      "type": "string"
    },
    "City": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-location.property-location"
    },
    "Latitude": {
      "type": "string"
    },
    "Longitude": {
      "type": "string"
    },
    "Commercial": {
      "type": "enumeration",
      "enum": [
        "Shops",
        "Showroom",
        "Plug and Play",
        "Office Space"
      ]
    },
    "Floor": {
      "type": "string"
    },
    "Age_of_Property": {
      "type": "string"
    },
    "Maintenance_Expected": {
      "type": "enumeration",
      "enum": [
        "No",
        "Yes"
      ]
    },
    "Amenities": {
      "type": "component",
      "repeatable": true,
      "component": "property.amenities"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Review",
        "Publish",
        "Removed from User",
        "Rejected from Admin"
      ]
    },
    "Main_Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Phone_Number": {
      "type": "string"
    },
    "user_website": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-website.user-website"
    },
    "Prop_Description": {
      "type": "richtext"
    },
    "User_Property_Facing": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-facing.property-facing"
    },
    "Address": {
      "type": "text"
    },
    "Place_Name": {
      "type": "string"
    },
    "Balcony": {
      "type": "string"
    },
    "Possession": {
      "type": "string"
    },
    "Power_Backup": {
      "type": "string"
    },
    "Maintenance_Charges": {
      "type": "string"
    },
    "Furnishing_Status": {
      "type": "string"
    },
    "Gated_Security": {
      "type": "string"
    },
    "Ownership_Type": {
      "type": "string"
    },
    "Flooring": {
      "type": "string"
    },
    "Carpet_Area": {
      "type": "string"
    },
    "Video_URL": {
      "type": "string"
    },
    "Brochure": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}

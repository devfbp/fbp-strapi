{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "required": true
    },
    "Posted_At": {
      "type": "date",
      "required": true
    },
    "Tags": {
      "displayName": "Blog_Tag",
      "type": "component",
      "repeatable": true,
      "component": "blog.blog-tag"
    },
    "Alias": {
      "type": "uid",
      "targetField": "Title"
    },
    "BlogCategory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-category.blog-category"
    },
    "ImageAltText": {
      "type": "string"
    },
    "ImageDescription": {
      "type": "text"
    },
    "MetaTitle": {
      "type": "string"
    },
    "MetaKeywords": {
      "type": "string"
    },
    "MetaDescription": {
      "type": "text"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}

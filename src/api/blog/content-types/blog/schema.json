{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "required": true
    },
    "Posted_At": {
      "type": "date",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Tags": {
      "displayName": "Blog_Tag",
      "type": "component",
      "repeatable": true,
      "component": "blog.blog-tag"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    }
  }
}
